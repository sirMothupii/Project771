<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mothupii_.project77.Mentorship">

    <ImageButton
        android:id="@+id/proffHelpBtn"
        android:layout_width="282dp"
        android:layout_height="173dp"
        android:layout_marginBottom="7dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:onClick="onClick1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.732"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/social_entr" />

    <TextView
        android:id="@+id/proffHelpTxt"
        android:layout_width="243dp"
        android:layout_height="40dp"
        android:text="Get Proffesional Help"
        android:textAlignment="center"
        android:textSize="24sp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/proffHelpBtn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.631"
        android:layout_marginBottom="0dp" />

    <WebView
        android:id="@+id/loadAnsWeb"
        android:layout_width="277dp"
        android:layout_height="189dp"
        android:layout_marginBottom="41dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="66dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="66dp"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/proffHelpTxt"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>
